# Porta do servidor Express
PORT=3000

# Configuração do MCP
MCP_SERVER_NAME=MCP Continuity Server
MCP_SERVER_VERSION=1.0.9

# URL base para o servidor
BASE_URL=http://localhost:3000

# Diretório de trabalho padrão (deixar em branco se não existir)
DEFAULT_WORKING_DIR=

# Configurações de log
LOG_LEVEL=info

# Configurações de autenticação básica
# Defina AUTH_ENABLED=true para ativar a autenticação
AUTH_ENABLED=false
AUTH_REALM=MCP Continuity Server
AUTH_USERNAME=admin
AUTH_PASSWORD=password
